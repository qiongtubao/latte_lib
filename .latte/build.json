{
	"keys": {

	},
	"build": [
		{
			"command": "merger",
			"types": "js",
			"in": ["./index.js", "./lib"],
			"out": "./build/{{version}}/{{name}}.js"
		},
		{
			"command": "copy",
			"mini": 1,
			"in": ["./build/{{version}}/{{name}}.js"],
			"out": "./build/{{version}}/{{name}}.min.js"
		},
		{
			"command": "copy",
			"type": "js",
			"in": ["./build/{{version}}/{{name}}.js"],
			"out": ["./build/{{name}}.js"]
		}

	]
}